<p id="notice"><%= notice %></p>

<%
file = ""
File.open("logg", "r") do |f|
  f.each_line do |line|
    file = line
  end
end
%>

<img width="350" height="350" src="<%= "/#{file.gsub("\n",'')}" %>">
